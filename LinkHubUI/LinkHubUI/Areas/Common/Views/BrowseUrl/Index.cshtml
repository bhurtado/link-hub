@model IEnumerable<BOL.Url>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>URLs</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UrlTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Url1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Category.CategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UrlTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Url1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrlDesc)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Category.CategoryName)
        </td>
        
    </tr>
}

</table>
